{
  "name": "flipboxdigital/spark",
  "description": "A plugin framework for Craft CMS",
  "version": "1.0.0-beta.5",
  "keywords": [
    "flipbox",
    "spark",
    "craft cms"
  ],
  "license": "MIT",
  "authors": [{
    "name": "Flipbox Factory",
    "email": "hello@flipboxfactory.com"
  }],
  "require": {
    "craftcms/cms": "~3.0.0-beta.21"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.0",
    "squizlabs/php_codesniffer": "^2.0"
  },
  "repositories": [{
    "type": "composer",
    "url": "https://asset-packagist.org"
  }],
  "autoload": {
    "psr-4": {
      "flipbox\\spark\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "flipbox\\spark\\tests\\": "tests/src/"
    }
  }
}
